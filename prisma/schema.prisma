generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Infiel {
  id          Int      @id @default(autoincrement())
  nombre      String
  edad        Int?     // El ? significa opcional
  ocupacion   String?
  ciudad      String
  descripcion String
  redSocial   String?
  iniciales   String?
  creadoEn    DateTime @default(now())
}